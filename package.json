{
  "name": "iobroker.soliscloud",
  "version": "1.4.3",
  "description": "Adapter for reading data from soliscloud",
  "author": "Trixx <trixxdev034@gmail.com>",
  "contributors": [
    "iobroker-community-adapters <iobroker-community-adapters@gmx.de>",
    "mcm1957 <mcm57@gmx.at>"
  ],
  "homepage": "https://github.com/iobroker-community-adapters/ioBroker.soliscloud",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "soliscloud",
    "Smart Home",
    "home automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iobroker-community-adapters/ioBroker.soliscloud.git"
  },
  "engines": {
    "node": ">= 20"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.3.2"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^5.0.0",
    "@alcalzone/release-script-plugin-iobroker": "^4.0.0",
    "@alcalzone/release-script-plugin-license": "^4.0.0",
    "@alcalzone/release-script-plugin-manual-review": "^4.0.0",
    "@iobroker/adapter-dev": "^1.5.0",
    "@iobroker/testing": "^5.2.2",
    "@tsconfig/node14": "^14.1.8",
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.1.1",
    "axios": "^1.13.4",
    "eslint": "^8.57.0",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~5.9.3"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "build-adapter ts",
    "postbuild": "npm run test:package",
    "watch": "build-adapter ts --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "dev-server": "dev-server",
    "watch:ts": "build-adapter ts --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts src/",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/iobroker-community-adapters/ioBroker.soliscloud/issues"
  },
  "readmeFilename": "README.md"
}
